{
  "tests": [
    {
      "group": "Example",
      "tests": [
        {
          "url": "http://www.example.com",
          "name": "root",
          "header": {
            "Accept": "text/html"
          },
          "mime": "text/html",
          "checks": [
            {
              "name": "containsNot",
              "term": "Lorem ipsum"
            },
            {
              "name": "contains",
              "term": "<h1>Example Domain</h1>"
            }
          ]
        },

        "http://www.example.net"

      ]
    },
    {
      "group": "LocalJson",
      "tests": [

        {
          "url": "http://localhost:8088/json-count-year.json",
          "checks": [
            "json",
            {
              "name": "jsonValue",
              "path": ".total",
              "gte": 40
            },
            {
              "name": "jsonValue",
              "path": ".unit",
              "equal": "\u20ac"
            },
            {
              "name": "jsonCount",
              "path": ".year",
              "equal": 2
            }
          ]
        }
        
      ]
    }
  ]
}
